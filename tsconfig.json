{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",                                /* Specify what module code is generated. */
    "rootDir": "./",
    "outDir": "./dist/js",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true, 
    "strict": true, 
    "baseUrl": "./",
    "noUnusedLocals": true, 
    "noUnusedParameters": true, 
    "skipLibCheck": true,
    "paths": {   
      "#Listener/*": ["Listener/*"] 
    }, 
  } 
}
