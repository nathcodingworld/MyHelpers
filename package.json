{
  "name": "helpersmodule",
  "version": "1.0.0",
  "description": "Creating my Own Helpers",
  "main": "index.js",
  "scripts": {
    "compileListener": "concurrently \"sass Listener\" \"tsc Listener\"",
    "activateListener": "concurrently \"sass --watch Listener\" \"tsc -w Listener/Listener.ts\"",
    "compileObserver": "concurrently \"sass Observer\" \"tsc Observer\"",
    "activateObserver": "concurrently \"sass --watch Observer\" \"tsc -w Observer/Observer.ts\"",
    "compileRenderer": "concurrently \"sass Renderer\" \"tsc Renderer\"",
    "activateRenderer": "concurrently \"sass --watch Renderer\" \"tsc -w Renderer/Renderer.ts\"",
    "compileBuilder": "concurrently \"sass Builder\" \"tsc Builder\"",
    "activateBuilder": "concurrently \"sass --watch Builder\" \"tsc -w Builder/Builder.ts\""
  },
  "keywords": [],
  "author": "Nathaniel M.",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.1.0",
    "gulp-uglify": "^3.0.2",
    "sass": "^1.62.1"
  }
}
